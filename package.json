{
	"name": "waas-app-api",
	"version": "1.0.0",
	"main": "server.js",
	"type": "module",
	"engines": {
		"node": ">=20.0.0"
	},
	"scripts": {
		"start": "cross-env NODE_ENV=production node ./src/server.js",
		"dev": "cross-env NODE_ENV=development nodemon ./src/server.js",
		"prodlike": "cross-env NODE_ENV=production nodemon ./src/server.js",
		"format": "prettier -w .",
		"format:check": "prettier -c .",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"lint:strict": "eslint --max-warnings=0 src",
		"test": "cross-env NODE_ENV=test mocha test/**/*.spec.js --bail --exit",
		"prepare": "husky || true"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/rheyhannh/waas-app-api.git"
	},
	"author": "Reyhan Naufal Hayyan",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/rheyhannh/waas-app-api/issues"
	},
	"homepage": "https://github.com/rheyhannh/waas-app-api#readme",
	"description": "",
	"dependencies": {
		"@fastify/env": "^5.0.1",
		"@fastify/rate-limit": "^10.2.1",
		"@fastify/static": "^8.0.3",
		"@ilovepdf/ilovepdf-nodejs": "^0.3.0",
		"dotenv": "^16.4.7",
		"fastify": "^5.2.0",
		"telegraf": "^4.16.3"
	},
	"devDependencies": {
		"@eslint/js": "^9.17.0",
		"chai": "^5.1.2",
		"chai-as-promised": "^8.0.1",
		"chai-http": "^5.1.1",
		"cross-env": "^7.0.3",
		"eslint": "^9.17.0",
		"globals": "^15.14.0",
		"lint-staged": "^15.3.0",
		"mocha": "^11.1.0",
		"nodemon": "^3.1.9",
		"pino-pretty": "^13.0.0",
		"prettier": "^3.4.2",
		"sinon": "^19.0.2",
		"supertest": "^7.0.0"
	},
	"volta": {
		"node": "22.12.0"
	},
	"lint-staged": {
		"src/**/*.js": [
			"eslint --max-warnings=0",
			"prettier -w"
		],
		"src/**/*.{json,md,mdx}": [
			"prettier -w"
		]
	}
}
